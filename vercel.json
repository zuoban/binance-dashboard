{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "regions": ["hkg1", "sin1"],
  "env": {
    "NEXT_PUBLIC_BINANCE_REST_API": "https://fapi.binance.com",
    "NEXT_PUBLIC_BINANCE_WS_API": "wss://fstream.binance.com/ws"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_BINANCE_REST_API": "https://fapi.binance.com",
      "NEXT_PUBLIC_BINANCE_WS_API": "wss://fstream.binance.com/ws"
    }
  },
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,DELETE,PATCH,POST,PUT"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, X-CSRF-Token, Authorization"
        }
      ]
    }
  ]
}
